{
  "name": "developmint.de",
  "version": "1.1.3",
  "description": "Our company website",
  "author": "Alexander Lichter <alichter@developmint.de>",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development nuxt",
    "build": "NODE_ENV=production nuxt build",
    "start": "NODE_ENV=production nuxt start",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "npm run lint && npm run build && bundlesize"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.1",
    "@nuxtjs/google-analytics": "^2.0.2",
    "@nuxtjs/pwa": "^2.0.8",
    "autoprefixer": "^9.0.0",
    "bundlesize": "^0.17.0",
    "express": "^4.16.3",
    "glob-all": "^3.1.0",
    "helmet": "^3.12.1",
    "intersection-observer": "^0.5.0",
    "node-sass": "^4.9.2",
    "nodemailer": "^4.6.7",
    "nuxt": "^1.4.1",
    "nuxt-i18n": "^4.1.0",
    "purgecss": "^1.0.1",
    "purgecss-webpack-plugin": "^0.23.0",
    "sass-loader": "^7.0.3",
    "shifty": "^2.3.1",
    "tailwindcss": "^0.6.4",
    "validator": "^10.4.0",
    "vue-analytics": "^5.12.2",
    "vue-cookieconsent-component": "^1.2.0",
    "vue-if-bot": "^1.1.2",
    "vue-meta": "^1.5.2",
    "vue-observe-visibility": "^0.4.1",
    "vuelidate": "^0.7.4",
    "xss-filters": "^1.2.7"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "eslint": "^5.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-plugin-vue": "^4.7.0",
    "husky": "^1.0.0-rc.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "bundlesize": [
    {
      "path": "./.nuxt/dist/+([0123456789]).*.js",
      "maxSize": "6.5 kB"
    },
    {
      "path": "./.nuxt/dist/+(lang-*).*.js",
      "maxSize": "10 kB"
    },
    {
      "path": "./.nuxt/dist/app.*.js",
      "maxSize": "15 kB"
    },
    {
      "path": "./.nuxt/dist/vendor*.js",
      "maxSize": "63 kB"
    }
  ]
}
